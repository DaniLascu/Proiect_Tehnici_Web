                    <h2>Bine ati venit</h2>
                    <a target="_blank" href="http://www.google.com">Google</a>
                    <figure id="imagini">
                        <picture>
                            <source srcset="/resurse/imagini/rolex1.jpg" media="(max-width: 1200px)"/>
                            <source srcset="/resurse/imagini/rolex2.jpg" media="(max-width:1700px)"/>
                            <img class="picture1" src="/resurse/imagini/rolex3.jpg" alt="imagine ceas" title="Ceas">
